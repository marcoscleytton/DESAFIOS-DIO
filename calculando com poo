# TODO: Crie uma classe e método para realizar a soma:
class Calculadora:
    def __init__(self):
        pass

    def soma(self, x, y):
        return x + y

num1 = int(input())
num2 = int(input())

# Criando uma instância da calculadora
calc = Calculadora()

resultado = calc.soma(num1, num2)
print(resultado)
